<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox spacing="10.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="taskautomation.trigger.TimeInputDialogController">

    <Label fx:id="label" text="Inserisci l'orario (hh:mm):" />
    <TextField fx:id="textField" />
   <HBox alignment="CENTER" prefHeight="0.0" prefWidth="161.0">
      <children>
          <Button fx:id="okButton" alignment="CENTER" text="OK" />
      </children>
   </HBox>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</VBox>
